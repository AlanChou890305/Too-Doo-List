{
  "cli": {
    "version": ">= 5.10.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal"
    },
    "staging": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "staging"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
