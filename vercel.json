{
  "version": 2,
  "name": "too-doo-list",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/auth/callback",
      "destination": "/auth/callback.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
