{
  "rewrites": [
    { "source": "/auth/callback", "destination": "/callback.html" },
    { "source": "/auth/callback-local.html", "destination": "/auth/callback-local.html" }
  ],
  "headers": [
    {
      "source": "/_expo/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
